    /**
     * Construtor que permite passar instância de uma equipa, preveem-se os seguintes cenários:
        - Consultar Equipa (editFlag=true);
        - Editar Equipa (editFlag=true);
     * @param e, Equipa sobre o qual se consultam ou editam dados.
     * @param vols, referência da instância Voluntarios para aceder a métodos.
     * @param editFlag, será true se é pretendido consultar/editar uma equipa, false caso contrário.
     */
    public RegistoEquipa(IEquipa e, Voluntarios vols, boolean editFlag){
        initComponents();
        this.addInternalFrameListener(new JCloseIFrameListener(this));
        
        this.eqAtual=e;
        this.parent=vols;
        this.editFlag=editFlag;

        if(editFlag==true){
            this.setTitle("Equipa: "+this.eqAtual.getDesignacao());
        } else{
            this.setTitle("Nova Equipa");
        }
        this.enameTextField.setText(this.eqAtual.getDesignacao());
        this.eNumberTextField.setText(String.valueOf(this.eqAtual.getId()));
        this.eorigTextField.setText(this.eqAtual.getpOrigem());
        this.echefeIdTextField.setText(String.valueOf(this.eqAtual.getId()));
        this.echefeNameTextField.setText(vols.vols.get(eqAtual.getId()).getFirstAndLastName());
        this.obsTextArea.setText(this.eqAtual.getObs());
        
        // Tabela de voluntários sem equipa
        DefaultTableModel model = (DefaultTableModel) this.evolsTable.getModel();
        int i=0;
        for(IVoluntario v : vols.vols.values()){ // Preencher tabela com dados de voluntários
            // select dos voluntarios que não têm equipa
            model.addRow(new Object[]{v.getNr(), v.getFirstAndLastName()});
        }
        
        // Tabela de voluntários desta equipa
        model = (DefaultTableModel) this.teamvolsTable.getModel();
        i=0;
        for(IVoluntario v : vols.vols.values()){ // Preencher tabela com dados de voluntários
            // select dos voluntários desta equipa
            model.addRow(new Object[]{v.getNr(), v.getFirstAndLastName()});
        }
    }

