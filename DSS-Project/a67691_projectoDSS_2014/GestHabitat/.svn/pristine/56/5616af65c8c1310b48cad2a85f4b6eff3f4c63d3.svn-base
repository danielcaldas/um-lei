    /**
     * Procedimentos a quando do clique no butão guardar.
     * @param evt, um evento.
     */
    private void onClickSaveButton(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_onClickSaveButton
        ArrayList<String> perms = new ArrayList<>();
        // Verificar se utilizador tem permissões para editar e criar
        if(/*this.parent.getVoluntariosParent().getLoggedFuncionario().hasPermissions(perms)*/true){
            // Instanciar novo OU editada equipa com campos lidos e passá-lo como parâmetro para a parent frame            
            IEquipa e = new EquipaFactory().createEquipa();
            
            // Ids dos voluntários desta equipa
            Set<Integer> ids = new HashSet<>();            
            int n = this.teamvolsTable.getRowCount();
            for(int i=0; i<n; i++){
                ids.add((int)this.teamvolsTable.getValueAt(i,0)); // gets ids na coluna 0
            }
            
            int idboss;
            try{
                idboss = Integer.parseInt(this.echefeIdTextField.getText().trim());
            } catch(NumberFormatException ex){idboss=0;}
            
            e.setVoluntarios(ids);
            e.setDesignacao(this.enameTextField.getText().trim());
            e.setpOrigem(this.eorigTextField.getText().trim());
            e.setObs(this.obsTextArea.getText().trim());

            if(this.parent.saveEquipaRegist(e)){
                this.setTitle("Equipa: "+e.getDesignacao());
            }
        } else{ new NoPermissionsDialog(this).show(); }
    }//GEN-LAST:event_onClickSaveButton

