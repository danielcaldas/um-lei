    /**
     * Procedimentos a quando do clique no butão delete.
     * @param evt , um evento.
     */
    private void onClickDeleteButton(java.awt.event.ActionEvent evt) {                                     
        if(/*this.parent.getVoluntariosParent().getLoggedFuncionario().hasDeletePermissions()*/ true){           
            if (this.eqAtual!=null){
                if(this.parent.vols.containsKey(eqAtual.getId())){
                    // Lançar diálogo
                    int option = new AreYouSureDeleteDialog(" a equipa "+this.eqAtual.getDesignacao()).show();
                    if(option == JOptionPane.YES_OPTION){ // Verificar se voluntário está na base de dados
                        this.parent.vols.remove(this.eqAtual.getId());
                        StringBuilder sb = new StringBuilder();
                        sb.append("Voluntário eliminado: ").append(this.eqAtual.getDesignacao()).append(" Idº:")
                          .append(this.eqAtual.getId()).append("\n");
                        sb.append("Funcionário: ").append(this.parent.getVoluntariosParent().getNomeFuncionario()).append("\n");
                        sb.append("Data: ").append(this.parent.getVoluntariosParent().getCurrentDateTime()).append("\n");
                        this.parent.getVoluntariosParent().report(sb.toString());
                        // Editar linha da tabela que corresponde ao voluntário editado
                        this.parent.eDeleteRow(this.eqAtual.getId());
                        this.dispose(); // Fechar janela se operação é concluída com sucesso
                    }                   
                }
            }
        } else{ new NoPermissionsDialog(this).show(); }
    }

